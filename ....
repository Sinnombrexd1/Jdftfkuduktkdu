local englishButton = Instance.new("TextButton")
englishButton.Size = UDim2.new(0.8, 0, 0.3, 0)
englishButton.Position = UDim2.new(0.1, 0, 0.55, 0)
englishButton.Text = "English"
englishButton.TextSize = 14
englishButton.BackgroundTransparency = 0.5
englishButton.BackgroundColor3 = grayishBlack
englishButton.TextColor3 = white
englishButton.Parent = configFrame
addCorner(englishButton, 10)

local langTweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local langTweenGoal = {Position = UDim2.new(0.35, 0, 0.15, 0)}
local langTween = TweenService:Create(configFrame, langTweenInfo, langTweenGoal)

configButton.MouseButton1Click:Connect(function()
    configFrame.Visible = not configFrame.Visible
    if configFrame.Visible then
        langTween:Play()
    end
end)

local keyBox = Instance.new("TextBox")
keyBox.Size = UDim2.new(0.6, 0, 0.1, 0)
